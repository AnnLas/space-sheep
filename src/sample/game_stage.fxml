                  <Label fx:id="heightLabel" text="Height:" />
               </children>
            </HBox>
            <Label fx:id="velocityLabel" alignment="CENTER" contentDisplay="CENTER" text="Label" />
            <Label fx:id="massLabel" text="Label" />
            <Label fx:id="fuelUsageLabel" text="Label" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
      <Pane fx:id="side_view_pane" prefHeight="200.0" prefWidth="200.0" />
   </children>
</GridPane>
